{
  "name": "ui-react-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "vite build",
    "build:test": "VITE_API_URL=http://127.0.0.1:9999 VITE_HARD_FAIL_ON_SCHEMA=1 vite build",
    "preview": "vite preview --strictPort --port 5174 --host",
    "preview:test": "VITE_API_URL=http://127.0.0.1:9999 VITE_HARD_FAIL_ON_SCHEMA=1 vite preview --strictPort --port 5174 --host",
    "dev": "vite --port 5174 --host",
    "dev:test": "VITE_API_URL=http://127.0.0.1:9999 VITE_HARD_FAIL_ON_SCHEMA=1 vite --port 5174 --host",
    "typecheck": "tsc -b",
    "lint": "echo 'ESLint config TODO - skipping for now'",
    "e2e": "playwright test",
    "e2e:runtime": "playwright test tests/e2e/*.no-runtime.spec.ts",
    "e2e:report": "playwright show-report",
    "test:golden": "npm run typecheck && npm run build:test && npm run e2e:runtime"
  },
  "devDependencies": {
    "@playwright/test": "^1.45.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.0",
    "eslint": "^8.57.0",
    "tsx": "^4.20.4",
    "typescript": "^5.6.0",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "zod": "^4.0.17"
  }
}
